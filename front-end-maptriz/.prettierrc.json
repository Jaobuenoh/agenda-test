{
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "htmlWhitespaceSensitivity": "css",
  "bracketSameLine": false,
  "singleAttributePerLine": false,
  "arrowParens": "always",
  "bracketSpacing": true,
  "cursorOffset": -1,
  "endOfLine": "crlf",
  "noImplicitAny": true,
  "insertPragma": false,
  "printWidth": 140,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
  "trailingComma": "all",
  "useTabs": false
}
